generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model Player {
  id        Int      @id @default(autoincrement())
  disc_tag String   @unique
  val_mmr Int
  total_val_games Int @default(0)
  rl_mmr Int
  total_rl_games Int @default(0)
  createdAt DateTime @default(now())
}
